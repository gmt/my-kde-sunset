+void QMultiInputContext::reset()
+{
+    if ( slave() )
+	slave()->reset();
+}
+
+
+void QMultiInputContext::setFocus()
+{
+    cachedFocus = TRUE;
+    if ( slave() )
+	slave()->setFocus();
